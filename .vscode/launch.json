    "version": "0.2.0",
    "configurations": [
        {
            "name": "[Chore Master API] Web Server",
            "type": "debugpy",
            "request": "launch",
            "module": "apps.chore_master_api.entrypoints.web_server",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1",
                "ENV": "local",
                "SERVICE_NAME": "chore_master_api",
                "COMPONENT_NAME": "web_server",
                "DATABASE_ORIGIN": "sqlite+aiosqlite:///./dbs/local.db",
                "DATABASE_SCHEMA_NAME": "",
            }
        },
        {
            "name": "[Bill Processor] Sinotrade",
            "type": "debugpy",
            "request": "launch",
            "module": "apps.bill_processor.entrypoints.sinotrade",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
                "API_KEY": "<API_KEY>",
                "SECRET_KEY": "<SECRET_KEY>",
            }
        },
        {
            "name": "[Bill Processor] Sinotrade Test",
            "type": "debugpy",
            "request": "launch",
            "module": "apps.bill_processor.entrypoints.sinotrade_test",
