            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
                "API_KEY": "<API_KEY>",
                "SECRET_KEY": "<SECRET_KEY>",
            }
        },
        {
            "name": "[Bill Processor] Build bill",
            "type": "debugpy",
            "request": "launch",
            "module": "apps.bill_processor.entrypoints.build_bill",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
            }
        },
        {
            "name": "[Bill Processor] Report pnl from bill",
            "type": "debugpy",
            "request": "launch",
            "module": "apps.bill_processor.entrypoints.report_pnl_from_bill",
            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
            }
        },
        {
            "name": "[Bill Processor] Report portfolio plot from bill",
            "type": "debugpy",
