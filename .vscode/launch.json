            "console": "integratedTerminal",
            "cwd": "${workspaceRoot}",
            "justMyCode": false,
            "env": {
              "ENV": "local",
              "SERVICE_NAME": "trading_bot",
              "COMPONENT_NAME": "strategy_manager",
            }
        },
    ]
}